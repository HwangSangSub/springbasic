<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.brighten.idi.delivery.mapper.DeliveryMapper">

	<select id="deliveryInfo" resultType="com.brighten.idi.delivery.dto.DeliveryDto">
		SELECT *
		FROM idi_delivery_info
		ORDER BY sno desc 
		LIMIT 1
	</select>

	
</mapper>	 